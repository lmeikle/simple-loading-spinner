{
  "name": "@lmeikle/simple-loading-spinner",
  "version": "1.1.1",
  "description": "Renders a simple react loading spinner",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lmeikle/simple-loading-spinner"
  },
  "author": "lmeikle",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "prepublish": "npm run build",
    "test": "jest --watch"
  },
  "dependencies": {
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.3",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^23.5.0",
    "react": "^16.4.2",
    "style-loader": "^0.22.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "keywords": [
    "simple react loading spinner"
  ],
  "jest": {
    "verbose": false,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ]
  }
}
